		Subscriber subsciber1 = new Subscriber("kavya");		
		Subscriber subsciber4 = new Subscriber("sunil");
		Subscriber subsciber2 = new Subscriber("priya");

		Subscriber subsciber = new Subscriber("swathi");
	
			channel.subscribe(subsciber1);
			channel.subscribe(subsciber2);
			channel.subscribe(subsciber3);
			channel.subscribe(subsciber4);
			
			


